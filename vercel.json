{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://cryptojobboard-production.up.railway.app/api/$1" },
    { "source": "/objects/(.*)", "destination": "https://cryptojobboard-production.up.railway.app/objects/$1" },
    { "source": "/public-objects/(.*)", "destination": "https://cryptojobboard-production.up.railway.app/public-objects/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
